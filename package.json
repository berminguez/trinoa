    "recharts": "2.15.4",
    "remark-gfm": "^4.0.1",
    "shadcn": "^2.9.3",
    "sharp": "0.32.6",
    "sonner": "^2.0.6",
    "stripe": "^18.3.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "textarea": "^0.3.0",
    "uuid": "^11.1.0",
    "vaul": "^1.1.2",
    "yup": "^1.7.0",
    "zod": "^3.24.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@playwright/test": "1.50.0",
    "@testing-library/react": "16.3.0",
    "@types/agenda": "^4.1.4",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^22.5.4",
