    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "@types/stripe": "^8.0.417",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.0",
    "eslint-plugin-import": "^2.32.0",
    "jsdom": "26.1.0",
    "pdf-lib": "^1.17.1",
    "playwright": "1.50.0",
    "playwright-core": "1.50.0",
