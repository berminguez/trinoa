{
  "name": "eidetik",
  "version": "1.0.0",
  "description": "Eidetik is a platform for creating and managing AI-powered content.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,mdx,css,scss,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,mdx,css,scss,yaml,yml}\"",
    "type-check": "tsc --noEmit",
    "code-quality": "pnpm run type-check && pnpm run lint && pnpm run format:check",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "test:queue": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/test-queue-system.ts",
    "test:api": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/test-api-endpoints.ts",
    "test:upload": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/test-upload-endpoint.ts",
    "test:resources": "cross-env NODE_OPTIONS=--no-deprecation tsx src/collections/Resources.test.ts",
    "test:upload-route": "cross-env NODE_OPTIONS=--no-deprecation tsx src/app/api/resources/upload/route.test.ts",
    "test:downloader": "cross-env NODE_OPTIONS=--no-deprecation tsx src/lib/video/downloader.test.ts",
    "test:transcriber": "cross-env NODE_OPTIONS=--no-deprecation tsx src/lib/video/transcriber.test.ts",
    "test:vision-describer": "cross-env NODE_OPTIONS=--no-deprecation tsx src/lib/video/vision-describer.test.ts",
    "test:media-uploader": "cross-env NODE_OPTIONS=--no-deprecation tsx src/lib/video/media-uploader.test.ts",
    "queue:monitor": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/queue-monitor.ts",
    "queue:monitor:detailed": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/queue-monitor.ts --detailed --cleanup"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.23",
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/lib-storage": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@dnd-kit/core": "^6.3.1",
